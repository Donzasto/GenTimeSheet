<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GenTimeSheet.Controls.Month">
		<Border Grid.Column="0" Grid.Row="0" BorderBrush="Aqua" BorderThickness="1" CornerRadius="3" Margin="1">
			<Grid RowDefinitions="auto, *">
				<Grid Grid.Row="0" RowDefinitions="*">
					<TextBlock Text="Январь" TextAlignment="Center"/>
				</Grid>
				<Grid Name="MonthDates" Grid.Row="1" ColumnDefinitions="*, *, *, *, *, *, *" RowDefinitions="*, *, *, *, *, *, *"/>
			</Grid>
		</Border>
</UserControl>
